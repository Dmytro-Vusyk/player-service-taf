{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "players": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "age": {
            "type": "integer",
            "format": "int32"
          },
          "gender": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "role": {
            "type": "string"
          },
          "screenName": {
            "type": "string"
          }
        },
        "required": ["age", "gender", "id", "role", "screenName"]
      }
    }
  },
  "required": ["players"]
}
